{
  "name": "vscodemod",
  "displayName": "vscodemod",

  "version": "1.0.0",
  "description": "a couple of misc codemods for fun and profit",
  "publisher": "nehima",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.vscodeModzz"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elliotberry/vscodemodz"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.vscodeModz",
        "title": "VSCodeMod: Run codemodz on selected code",
        "description": "Run JavaScript codemods through jscodeshift"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.2.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",

    "vsce": "^2.15.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "conventional-changelog-cli": "^5.0.0",
    "jscodeshift": "^17.0.0"
  }
}
